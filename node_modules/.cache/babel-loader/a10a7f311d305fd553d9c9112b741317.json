ass=\"v-pass-recovery\">\n    <form @submit.prevent=\"onSubmit\" class=\"v-pass-recovery__form\">\n      <h2 class=\"v-pass-recovery__title\">\n        Восстановление пароля\n      </h2>\n      <div class=\"v-pass-recovery__input\">\n        <BaseInputText\n          class=\"v-pass-recovery__field\"\n          icon=\"email\"\n          placeholder=\"Ваша почта\"\n          v-model=\"email\"\n          :error=\"error_email\"\n          :error_message=\"error_message\"\n        />\n      </div>\n      <button class=\"v-pass-recovery__btn\">\n        <span>Отправить письмо</span>\n      </button>\n    </form>\n  </div>\n</template>\n\n<script>\nimport {mapMutations} from \"vuex\";\n\nexport default {\n  name: \"PasswordRecovery\",\n  data() {\n    return {\n      email: \"\",\n      error_email: false,\n      error_message: null\n    };\n  },\n  methods: {\n    ...mapMutations([\"SHOW_MODAL\"]),\n    onSubmit() {\n      const sendingData = {};\n      if (\n        this.email.length === 0 ||\n        !this.email.match(/^[0-9a-z-.]+@[0-9a-z-]{2,}\\.[a-z]{2,}$/i)\n      ) {\n        this.error_email = true;\n        this.error_message = \"Некорректный email\";\n        setTimeout(() => {\n          this.error_email = false;\n          this.error_message = null;\n        }, 2000);\n        return;\n      } else {\n        sendingData.email = this.email;\n      }\n\n      this.$store.dispatch(\"auth/query_to_reset_password\", sendingData);\n      this.SHOW_MODAL(\"check\");\n    }\n  }\n};\n</script>\n\n<style lang=\"scss\">\n.v-pass-recovery {\n  padding-top: 178px;\n  &__form {\n    width: 311px;\n    margin: 0 auto;\n  }\n  &__title {\n    @extend %h2-title-regular;\n    color: $black;\n    margin-bottom: 23px;\n  }\n  &__input {\n    margin-bottom: 34px;\n  }\n  &__btn {\n    @extend %simple-btn-green;\n    width: 100%;\n  }\n}\n</style>\n"],"sourceRoot":"src/views"}]}