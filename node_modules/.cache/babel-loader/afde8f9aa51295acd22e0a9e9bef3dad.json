AA,aAAA,GAAA;AAFA,OAAA;AAIA,KAZA;AAaA,IAAA,KAbA,mBAaA;AACA,UAAA,OAAA,GAAA,EAAA;AACA,UAAA,IAAA,GAAA,KAAA,IAAA,GAAA,EAAA,cAAA,KAAA,IAAA,IAAA,KAAA,IAAA;;AAEA,WAAA,IAAA,CAAA,GAAA,KAAA,WAAA,EAAA,CAAA,IAAA,KAAA,SAAA,EAAA,CAAA,IAAA,CAAA,EAAA;AACA,YAAA,CAAA,KAAA,EAAA,EAAA;AACA;AACA;;AACA,YAAA,MAAA,GAAA,CAAA,GAAA,EAAA,cAAA,CAAA,IAAA,CAAA;AAEA,QAAA,OAAA,CAAA,IAAA,WAAA,IAAA,cAAA,MAAA;AACA;;AAEA,aAAA,OAAA;AACA,KA3BA;AA4BA,IAAA,cA5BA,4BA4BA;AACA,UAAA,IAAA,aAAA,KAAA,WAAA,KAAA,CAAA,GAAA,CAAA,GAAA,KAAA,WAAA,gBACA,KAAA,SADA,CAAA;AAGA,oCAAA,IAAA;AACA,KAjCA;AAkCA,IAAA,iBAlCA,+BAkCA;AACA,aAAA,KAAA,SAAA,GAAA,EAAA,0CAAA,EAAA;AACA;AApCA,GAzBA;AA+DA,EAAA,IA/DA,kBA+DA;AACA,WAAA;AACA,MAAA,KAAA,EAAA;AADA,KAAA;AAGA,GAnEA;AAoEA,EAAA,OAAA,EAAA;AACA,IAAA,eADA,6BACA;AACA,WAAA,KAAA,CAAA,OAAA,EAAA,IAAA;AACA,KAHA;AAIA,IAAA,YAJA,0BAIA;AAAA,UACA,IADA,GACA,IADA,CACA,IADA;AAAA,UACA,IADA,GACA,IADA,CACA,IADA;AAAA,UACA,KADA,GACA,IADA,CACA,KADA;AAAA,UACA,WADA,GACA,IADA,CACA,WADA,EAEA;;AACA,WAAA,KAAA,CAAA,cAAA,EAAA;AACA,QAAA,YAAA,EAAA,IADA;AAEA,QAAA,SAAA,YAAA,IAAA,cAAA,IAAA,GAAA,EAAA,GAAA,IAAA,cAAA,IAAA,CAAA,cAAA,WAAA,GAAA,EAAA,GAAA,WAAA,cAAA,WAAA,CAAA,QAFA;AAGA,QAAA,OAAA,YAAA,IAAA,cAAA,KAAA;AAHA,OAAA;AAKA;AAZA;AApEA,CAAA","sourcesContent":["<template>\n  <div\n    :style=\"componentStyle\"\n    :class=\"`${classNameForSmall} c-teacher-calendar__timeline_empty`\"\n    @mouseover=\"mouserOverEvent(minutesFrom, minutesTo)\"\n  >\n    <vue-slider\n      :tooltip=\"'none'\"\n      :process-style=\"{ background: '#EF4036' }\"\n      :step-active-style=\"{ background: 'transparent' }\"\n      :stepStyle=\"{ background: 'transparent' }\"\n      :rail-style=\"{ background: '#FBFBFB', height: mobile_style }\"\n      :dot-style=\"styles\"\n      @change=\"change_value\"\n      :disabled=\"!is_edit\"\n      :dotSize=\"is_mobile\"\n      v-model=\"value\"\n      :data=\"items\"\n      :marks=\"true\"\n    />\n  </div>\n</template>\n\n<script>\nimport \"vue-slider-component/theme/default.css\";\nimport VueSlider from \"vue-slider-component\";\n\nexport default {\n  name: \"SliderTeacher\",\n  components: {\n    VueSlider\n  },\n  props: {\n    hour: {\n      type: Number,\n      required: true\n    },\n    minutesFrom: {\n      type: Number,\n      required: true\n    },\n    minutesTo: {\n      type: Number,\n      required: true\n    },\n    is_edit: {\n      type: Boolean,\n    },\n    date: {\n      type: String\n    }\n  },\n  computed: {\n    is_mobile() {\n      return window.innerWidth > 1100 ? 10 : 15\n    },\n    mobile_style() {\n      return window.innerWidth > 1100 ? '10px' : '15px'\n    },\n    styles() {\n      return {\n        'box-shadow': 'none',\n        'background-color': !this.is_edit ? 'transparent' : '#ccc'\n      }\n    },\n    items() {\n      const minutes = [];\n      const hour = this.hour < 10 ? `0${this.hour}` : this.hour;\n\n      for (let j = this.minutesFrom; j <= this.minutesTo; j += 5) {\n        if (j === 60) {\n          continue;\n        }\n        const minute = j < 10 ? `0${j}` : j;\n\n        minutes.push(`${hour}:${minute}`);\n      }\n\n      return minutes;\n    },\n    componentStyle() {\n      const grid = `${this.minutesFrom === 0 ? 1 : this.minutesFrom} / ${\n        this.minutesTo\n      }`;\n      return `grid-column: ${grid}`;\n    },\n    classNameForSmall() {\n      return this.minutesTo < 60 ? `c-teacher-calendar__timeline_small` : \"\";\n    }\n  },\n  data() {\n    return {\n      value: null\n    };\n  },\n  methods: {\n    mouserOverEvent() {\n      this.$emit(\"hover\", null);\n    },\n    change_value() {\n      const { date, hour, value, minutesFrom } = this;\n      // console.log()\n      this.$emit(\"change_value\", {\n        calendar_day: date,\n        full_from: `${date} ${hour > 10 ? hour : `0${hour}`}:${minutesFrom > 10 ? minutesFrom : `0${minutesFrom}`}:00`,\n        full_to: `${date} ${value}:00`,\n      })\n    }\n  }\n};\n</script>\n"],"sourceRoot":"src/components"}]}