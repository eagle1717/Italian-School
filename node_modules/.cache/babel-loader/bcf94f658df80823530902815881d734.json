n        for (let j = this.minutesFrom; j <= this.minutesTo; j += 5) {\n          if (j === 60) {\n            continue;\n          }\n          const minute = j < 10 ? `0${j}` : j;\n\n          minutes.push(`${hour}:${minute}`);\n        }\n\n        return minutes;\n      },\n      timeEnd() {\n        return this.items.slice(-1)[0];\n      },\n      componentStyle() {\n        const grid = `${this.minutesFrom === 0 ? 1 : this.minutesFrom} / ${\n          this.minutesTo\n        }`;\n        return `grid-column: ${grid}`;\n      },\n      classNameForSmall() {\n        return this.minutesTo < 60 ? `c-teacher-calendar__timeline_small` : \"\";\n      }\n    },\n    data() {\n      return {\n        value: ''\n      };\n    },\n    methods: {\n      mouserOverEvent() {\n        this.$emit(\"hover\", null);\n      },\n    }\n  };\n</script>\n"],"sourceRoot":"src/components"}]}