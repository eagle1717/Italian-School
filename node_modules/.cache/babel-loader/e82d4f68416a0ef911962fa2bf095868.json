CfA,QAAAA,WAAW,CAACkB,MAAZ;AACAlB,QAAAA,WAAW,GAAG,IAAd;AACD;AACF,KALD;;AAOAF,IAAAA,EAAE,CAACqB,UAAH,GAAgBD,MAAhB;AACApB,IAAAA,EAAE,CAACsB,OAAH,GAAaF,MAAb;AACD;AAlCsB,CAAzB","sourcesContent":["import Vue from \"vue\";\n\nVue.directive(\"tooltip\", {\n  inserted: function(el, binding) {\n    let tooltipElem;\n\n    el.onmouseover = function() {\n      tooltipElem = document.createElement(\"div\");\n      tooltipElem.className = \"c-tooltip\";\n      tooltipElem.innerHTML = binding.value.text;\n      document.body.append(tooltipElem);\n\n      let coords = el.getBoundingClientRect();\n\n      let left =\n        coords.left + el.offsetWidth / 2 - tooltipElem.offsetWidth / 2 - 3;\n      if (left < 0) left = 0;\n\n      let top = coords.top - tooltipElem.offsetHeight - 10;\n      if (top < 0) {\n        top = coords.top + el.offsetHeight + 10;\n      }\n\n      tooltipElem.style.left = parseInt(left) + \"px\";\n      tooltipElem.style.top = parseInt(top) + \"px\";\n    };\n\n    const remove = () => {\n      if (tooltipElem) {\n        tooltipElem.remove();\n        tooltipElem = null;\n      }\n    };\n\n    el.onmouseout = remove;\n    el.onclick = remove;\n  }\n});\n"]}]}