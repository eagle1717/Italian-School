UAAA,GAAA,KAAA,UAAA;AACA;;AACA,UACA,KAAA,KAAA,CAAA,MAAA,KAAA,CAAA,IACA,CAAA,KAAA,KAAA,CAAA,KAAA,CAAA,yCAAA,CAFA,EAGA;AACA,aAAA,WAAA,GAAA,IAAA;AACA,aAAA,mBAAA,GAAA,oBAAA;AACA,QAAA,UAAA,CAAA,YAAA;AACA,UAAA,KAAA,CAAA,WAAA,GAAA,KAAA;AACA,UAAA,KAAA,CAAA,mBAAA,GAAA,IAAA;AACA,SAHA,EAGA,IAHA,CAAA;AAIA;AACA,OAXA,MAWA;AACA,QAAA,YAAA,CAAA,KAAA,GAAA,KAAA,KAAA;AACA;;AACA,UAAA,KAAA,QAAA,CAAA,MAAA,KAAA,CAAA,IAAA,KAAA,QAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,aAAA,cAAA,GAAA,IAAA;AACA,aAAA,sBAAA,GACA,kDADA;AAEA,QAAA,UAAA,CAAA,YAAA;AACA,UAAA,KAAA,CAAA,cAAA,GAAA,KAAA;AACA,UAAA,KAAA,CAAA,sBAAA,GAAA,IAAA;AACA,SAHA,EAGA,IAHA,CAAA;AAIA;AACA,OATA,MASA;AACA,QAAA,YAAA,CAAA,QAAA,GAAA,KAAA,QAAA;AACA;;AAEA,WAAA,MAAA,CACA,QADA,CACA,eADA,EACA,YADA,EAEA,IAFA,CAEA,YAAA;AACA,QAAA,KAAA,CAAA,UAAA,CAAA,OAAA;AACA,OAJA,EAKA,KALA,CAKA,UAAA,UAAA,EAAA;AACA,YAAA,UAAA,KAAA,GAAA,EAAA;AACA,UAAA,KAAA,CAAA,WAAA,GAAA,IAAA;AACA,UAAA,KAAA,CAAA,mBAAA,GACA,0CADA;AAEA,UAAA,UAAA,CAAA,YAAA;AACA,YAAA,KAAA,CAAA,WAAA,GAAA,KAAA;AACA,YAAA,KAAA,CAAA,mBAAA,GAAA,IAAA;AACA,WAHA,EAGA,IAHA,CAAA;AAIA;AACA,OAfA;AAgBA,KAtDA;AAuDA,IAAA,KAvDA,mBAuDA;AACA,WAAA,UAAA,CAAA,OAAA;AACA;AAzDA;AAnBA,CAAA","sourcesContent":["<template>\n  <BaseModal>\n    <div class=\"c-register-modal\">\n      <div class=\"c-register-modal__login\">\n        <button class=\"c-register-modal__login-btn\" @click=\"login\">\n          Войти\n        </button>\n      </div>\n      <form @submit.prevent=\"onSubmit\" class=\"c-register-modal__content\">\n        <h2 class=\"c-register-modal__title\">Быстрая регистрация</h2>\n        <p class=\"c-register-modal__subtitle\">\n          Для доступа к функциям личного кабинета необходимо зарегистрироваться.\n        </p>\n        <BaseInputText\n          class=\"c-register-modal__field\"\n          icon=\"user\"\n          placeholder=\"Ваше имя\"\n          v-model=\"first_name\"\n          :error=\"error_name\"\n        />\n        <BaseInputText\n          class=\"c-register-modal__field\"\n          icon=\"email\"\n          placeholder=\"Ваша почта\"\n          v-model=\"email\"\n          :error=\"error_email\"\n          :error_message=\"error_email_message\"\n        />\n        <BaseInputPassword\n          class=\"c-register-modal__field\"\n          placeholder=\"Придумайте пароль\"\n          :showProgress=\"true\"\n          v-model=\"password\"\n          :error=\"error_password\"\n          :error_message=\"error_password_message\"\n        />\n        <button class=\"c-register-modal__register-btn\">\n          <span>Зарегистрироваться</span>\n        </button>\n        <div class=\"c-register-modal__social-register\">\n          <p class=\"c-register-modal__social-tag\">Регистрация через:</p>\n          <div class=\"c-register-modal__socials\">\n            <button\n              class=\"c-register-modal__social c-register-modal__social--google\"\n            ></button>\n            <button\n              class=\"c-register-modal__social c-register-modal__social--facebook\"\n            ></button>\n            <button\n              class=\"c-register-modal__social c-register-modal__social--vk\"\n            ></button>\n          </div>\n        </div>\n      </form>\n    </div>\n  </BaseModal>\n</template>\n\n<script>\nimport { mapMutations } from \"vuex\";\n\nexport default {\n  name: \"RegisterModal\",\n  data() {\n    return {\n      first_name: \"\",\n      email: \"\",\n      password: \"\",\n      error_name: false,\n      error_email: false,\n      error_password: false,\n      error_email_message: null,\n      error_password_message: null\n    };\n  },\n  computed: {\n    isMobile() {\n      return window.innerWidth < 1100;\n    }\n  },\n  methods: {\n    ...mapMutations([\"SHOW_MODAL\"]),\n    onSubmit() {\n      const registerData = {};\n      if (this.first_name.length === 0) {\n        this.error_name = true;\n        setTimeout(() => (this.error_name = false), 2000);\n        return;\n      } else {\n        registerData.first_name = this.first_name;\n      }\n      if (\n        this.email.length === 0 ||\n        !this.email.match(/^[0-9a-z-.]+@[0-9a-z-]{2,}\\.[a-z]{2,}$/i)\n      ) {\n        this.error_email = true;\n        this.error_email_message = \"Некорректный email\";\n        setTimeout(() => {\n          this.error_email = false;\n          this.error_email_message = null;\n        }, 2000);\n        return;\n      } else {\n        registerData.email = this.email;\n      }\n      if (this.password.length === 0 || this.password.length < 6) {\n        this.error_password = true;\n        this.error_password_message =\n          \"Ненадежный пароль (Используйте символы A-z, 0-9)\";\n        setTimeout(() => {\n          this.error_password = false;\n          this.error_password_message = null;\n        }, 2000);\n        return;\n      } else {\n        registerData.password = this.password;\n      }\n\n      this.$store\n        .dispatch(\"auth/register\", registerData)\n        .then(() => {\n          this.SHOW_MODAL(\"check\");\n        })\n        .catch(statusCode => {\n          if (statusCode === 422) {\n            this.error_email = true;\n            this.error_email_message =\n              \"На эту почту уже зарегистрирован аккаунт\";\n            setTimeout(() => {\n              this.error_email = false;\n              this.error_email_message = null;\n            }, 3000);\n          }\n        });\n    },\n    login() {\n      this.SHOW_MODAL(\"login\");\n    }\n  }\n};\n</script>\n\n<style lang=\"scss\">\n.c-register-modal {\n  padding: rem(20) rem(116) 0 rem(114);\n  @extend %h100;\n\n  &__login {\n    @extend %df;\n    @extend %jcfe;\n    &-btn {\n      @extend %simple-btn-gray;\n      width: rem(133);\n      height: rem(45);\n    }\n  }\n\n  &__field {\n    width: 100%;\n    margin-bottom: rem(16);\n  }\n\n  &__content {\n    @extend %df;\n    @extend %fdc;\n    // @extend %jcc;\n    @extend %h100;\n    width: rem(311);\n    padding-top: rem(78);\n  }\n\n  &__title {\n    @extend %h2-title-regular;\n    color: $black;\n  }\n\n  &__subtitle {\n    @extend %text-small-big-line-height;\n    color: $black;\n    max-width: rem(300);\n    margin: rem(5) 0 rem(37) 0;\n  }\n\n  &__register-btn {\n    @extend %btn-border-green;\n    @extend %btn-all-green;\n    width: rem(311);\n    margin-top: rem(20);\n  }\n\n  &__social {\n    @extend %empt-btn;\n    width: 30px;\n    height: 30px;\n    border-radius: 5px;\n    background-repeat: no-repeat;\n\n    &-register {\n      text-align: center;\n      width: rem(311);\n    }\n\n    &:not(:last-child) {\n      margin-right: 11.53px;\n    }\n\n    &-tag {\n      margin-top: rem(40);\n      margin-bottom: rem(10);\n      @extend %ls-5;\n      @extend %regular;\n    }\n\n    &--google {\n      background-image: url($img_dir+\"/icons/google-auth.svg\");\n    }\n\n    &--facebook {\n      background-image: url($img_dir+\"/icons/fb-auth.svg\");\n    }\n\n    &--vk {\n      background-image: url($img_dir+\"/icons/vk-auth.svg\");\n    }\n\n    &__socials {\n      @extend %df;\n      @extend %aic;\n    }\n  }\n}\n@include bp(1100px) {\n  .c-register-modal {\n    padding: 20px 0 0 0;\n    max-width: 750px;\n    margin: 0 auto;\n    &__login {\n      display: flex;\n      align-items: center;\n      justify-content: flex-end;\n    }\n    &__content {\n      margin: 0 auto;\n    }\n  }\n}\n</style>\n"],"sourceRoot":"src/components/Modals"}]}