,YAAA,MAAA,GAAA,CAAA,GAAA,EAAA,cAAA,CAAA,IAAA,CAAA;AAEA,QAAA,OAAA,CAAA,IAAA,WAAA,IAAA,cAAA,MAAA;AACA;;AAEA,aAAA,OAAA;AACA,KAZA;AAaA,IAAA,OAbA,qBAaA;AACA,aAAA,KAAA,KAAA,CAAA,KAAA,CAAA,CAAA,CAAA,EAAA,CAAA,CAAA;AACA,KAfA;AAgBA,IAAA,aAhBA,2BAgBA;AACA,aAAA,KAAA,SAAA,KAAA,EAAA;AACA,KAlBA;AAmBA,IAAA,cAnBA,4BAmBA;AACA,UAAA,WAAA,aAAA,KAAA,WAAA,KAAA,CAAA,GAAA,CAAA,GAAA,KAAA,WAAA,gBACA,KAAA,SADA,CAAA;AAIA,oCAAA,WAAA;AACA,KAzBA;AA0BA,IAAA,iBA1BA,+BA0BA;AACA,UAAA,YAAA,GACA,KAAA,SAAA,GAAA,EAAA,0CAAA,EADA;AAEA,aAAA,YAAA;AACA;AA9BA,GAzBA;AAyDA,EAAA,OAAA,EAAA;AACA,IAAA,eADA,6BACA;AACA,WAAA,KAAA,CAAA,OAAA,EAAA,KAAA,EAAA;AACA,KAHA;AAIA,IAAA,SAJA,uBAIA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,UAAA;AACA,WAAA,MAAA,CAAA,QAAA,CAAA,+BAAA,EAAA;AACA,QAAA,IAAA,oBAAA,KAAA,UAAA,CADA;AAEA,QAAA,UAAA,EAAA;AAFA,OAAA;AAIA;AAVA;AAzDA,CAAA","sourcesContent":["<template>\n  <div\n    :style=\"componentStyle\"\n    :class=\"classNameForSmall\"\n    @mouseover=\"mouserOverEvent(minutesFrom, minutesTo)\"\n  >\n    <template v-if=\"isUserVisible\">\n      <div class=\"c-teacher-calendar__user\" :class=\"{'empty':!modal_data.student_first_name}\" @click=\"showModal\">\n        <template v-if=\"modal_data.student_first_name\">\n          <figure class=\"c-teacher-calendar__user-img\">\n            <template v-if=\"modal_data.photo\">\n              <img :src=\"modal_data.photo\" :alt=\"name\" />\n            </template>\n            <template v-else>\n              <img src=\"/img/ill/default-photo.svg\" :alt=\"name\" />\n            </template>\n          </figure>\n          <div class=\"c-teacher-calendar__user-content\">\n            <h3 class=\"c-teacher-calendar__user-name\">\n              {{ modal_data.student_first_name }}\n              {{ modal_data.student_last_name }}\n            </h3>\n            <button class=\"c-teacher-calendar__user-link\">\n              Перейти к занятию\n            </button>\n          </div>\n        </template>\n      </div>\n    </template>\n    <vue-slider\n      :tooltip=\"'none'\"\n      class=\"c-reserved-teacher-time\"\n      :process-style=\"{ background: '#39B100' }\"\n      :step-active-style=\"{ background: 'transparent' }\"\n      :stepStyle=\"{ background: 'transparent' }\"\n      :dot-style=\"{ display: 'none' }\"\n      :value=\"timeEnd\"\n      :data=\"items\"\n      :marks=\"true\"\n    />\n  </div>\n</template>\n\n<script>\nimport \"vue-slider-component/theme/default.css\";\nimport VueSlider from \"vue-slider-component\";\n\nexport default {\n  name: \"ReservedTeacherTime\",\n  components: {\n    VueSlider\n  },\n  props: {\n    modal_data: {\n      type: Object\n    },\n    minutesFrom: {\n      type: Number,\n      required: true\n    },\n    minutesTo: {\n      type: Number,\n      required: true\n    },\n    hour: {\n      type: Number,\n      required: true\n    },\n    id: {\n      type: Number\n    },\n  },\n  computed: {\n    items() {\n      const minutes = [];\n      const hour = this.hour < 10 ? `0${this.hour}` : this.hour;\n\n      for (let j = this.minutesFrom; j < this.minutesTo; j += 5) {\n        const minute = j < 10 ? `0${j}` : j;\n\n        minutes.push(`${hour}:${minute}`);\n      }\n\n      return minutes;\n    },\n    timeEnd() {\n      return this.items.slice(-1)[0];\n    },\n    isUserVisible() {\n      return this.minutesTo === 60;\n    },\n    componentStyle() {\n      const grid_column = `${this.minutesFrom === 0 ? 1 : this.minutesFrom} / ${\n        this.minutesTo\n      }`;\n\n      return `grid-column: ${grid_column};`;\n    },\n    classNameForSmall() {\n      const margin_class =\n        this.minutesTo < 60 ? `c-teacher-calendar__timeline_small` : \"\";\n      return margin_class;\n    }\n  },\n  methods: {\n    mouserOverEvent() {\n      this.$emit(\"hover\", this.id);\n    },\n    showModal() {\n      console.log(this.modal_data)\n      this.$store.dispatch('show_reservation_lesson_modal', {\n        data: {...this.modal_data},\n        modal_name: 'new-teacher-lesson'\n      })\n    },\n  }\n};\n</script>\n\n<style lang=\"scss\">\n.c-reserved-teacher-time {\n  pointer-events: none;\n}\n</style>\n"],"sourceRoot":"src/components"}]}