     <template v-if=\"!user.level.name\">\n        <p class=\"c-user-level__text\">\n          Ваш уровень не<br />\n          подтвержден\n        </p>\n        <router-link :to=\"{ name: 'FindOutLevel' }\" class=\"c-user-level__link\">\n          Выбрать уровень\n        </router-link>\n      </template>\n      <template v-else>\n        <figure class=\"c-user-level__img\">\n          <img\n            :src=\"`img/ill/level/${user.level.name}.svg`\"\n            :alt=\"user.level.name\"\n          />\n        </figure>\n      </template>\n    </div>\n    <!-- эти данные есть, другое наименование приходит с бека -->\n    <div class=\"c-user-level__wrap c-user-level__wrap_right\">\n      <p class=\"c-user-level__wrap-item\">Грамматика</p>\n      <CircleProgress :percent=\"user.level.grammar_percent\" />\n    </div>\n    <!-- эти данные есть, другое наименование приходит с бека -->\n    <div class=\"c-user-level__wrap c-user-level__wrap_right\">\n      <p class=\"c-user-level__wrap-item\">Лексика</p>\n      <CircleProgress :percent=\"user.level.vocabulary_percent\" />\n    </div>\n  </div>\n</template>\n\n<script>\nimport CircleProgress from \"../CircleProgress\";\nimport { mapState } from \"vuex\";\nexport default {\n  components: { CircleProgress },\n  data() {\n    return {\n      isCheckLevel: false\n    };\n  },\n  computed: {\n    ...mapState({ user: state => state.user.user })\n  }\n};\n</script>\n\n<style lang=\"scss\">\n.c-user-level {\n  @extend %df;\n  @extend %jcsa;\n  @extend %aic;\n  height: rem(150);\n  width: rem(430);\n  border: 1px solid #4b4b4b;\n  border-radius: 2px;\n  &__title {\n    @extend %text-big;\n    color: $black;\n  }\n  &__title,\n  &__wrap-item {\n    margin-bottom: rem(17);\n  }\n  &__text {\n    @extend %text-small-big-line-height;\n    color: $gray;\n  }\n  &__link {\n    @extend %dib;\n    margin-top: 7px;\n    @extend %btn-text;\n    color: $red;\n  }\n  &__wrap {\n    &-item {\n      @extend %text-small;\n    }\n  }\n}\n@include bp(1360px) {\n  .c-user-level {\n    margin-right: 20px;\n  }\n}\n@include bp(1100px) {\n  .c-user-level {\n    width: calc(100% / 2);\n    margin-right: 20px;\n  }\n}\n@include bp(766px) {\n  .c-user-level {\n    width: 100%;\n    margin-right: 0;\n    margin-bottom: 13px;\n    padding-left: 18px;\n    align-items: flex-start;\n    padding-top: 35px;\n    &__img {\n      width: 80px;\n    }\n    &__title {\n      font-size: rem(14);\n    }\n  }\n}\n</style>\n"]}]}