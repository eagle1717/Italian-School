    @click=\"openModal(l)\"\n        v-for=\"l in lessons\"\n        :key=\"l.title\"\n      >\n        <img src=\"/img/ill/user-group.png\" alt=\"\" />\n        <p class=\"name\">{{ l.teacher.first_name }} {{ l.teacher.last_name }}</p>\n        <h3 class=\"title\">{{ l.title }}</h3>\n        <hr />\n        <!-- <span class=\"schedule\">график занятий</span>\n        <span class=\"times\">Пн. 10:00 - 11:00</span>\n        <span class=\"times\">Ср. 12:00 - 13:00</span> -->\n        <a class=\"goToLesson\" href=\"#\" @click.prevent=\"openModal(l)\"\n          >Перейти к занятию</a\n        >\n      </div>\n      <!-- <div class=\"group-card\" @click=\"openModal\">\n        <img src=\"/img/ill/user-group.png\" alt=\"\">\n        <p class=\"name\">Robert Fox</p>\n        <h3 class=\"title\">Итальянский для чайников и тех кто его не знает.</h3>\n        <hr>\n        <span class=\"schedule\">график занятий</span>\n        <span class=\"times\">Пн. 10:00 - 11:00</span>\n        <span class=\"times\">Ср. 12:00 - 13:00</span>\n      </div>\n      <div class=\"group-card\" @click=\"openModal\">\n        <img src=\"/img/ill/user-group.png\" alt=\"\">\n        <p class=\"name\">Robert Fox</p>\n        <h3 class=\"title\">Итальянский для чайников и тех кто его не знает.</h3>\n        <hr>\n        <span class=\"schedule\">график занятий</span>\n        <span class=\"times\">Пн. 10:00 - 11:00</span>\n        <span class=\"times\">Ср. 12:00 - 13:00</span>\n      </div>\n      <div class=\"group-card\" @click=\"openModal\">\n        <img src=\"/img/ill/user-group.png\" alt=\"\">\n        <p class=\"name\">Robert Fox</p>\n        <h3 class=\"title\">Итальянский для чайников и тех кто его не знает.</h3>\n        <hr>\n        <span class=\"schedule\">график занятий</span>\n        <span class=\"times\">Пн. 10:00 - 11:00</span>\n        <span class=\"times\">Ср. 12:00 - 13:00</span>\n      </div> -->\n    </div>\n\n    <!-- <div class=\"teachers__wrap teachers__wrap_tech\">\n      <template v-if=\"curators_list.length > 0\">\n        <TeacherCard\n          v-for=\"itm of curators_list\"\n          :curator=\"itm\"\n          :key=\"itm.id\"\n        ></TeacherCard>\n      </template>\n      <template v-else>\n        <h3 class=\"teachers__not-found\">\n          По выбранным фильтрам учителей не найдено\n        </h3>\n      </template>\n    </div>\n    <Notifications /> -->\n  </div>\n</template>\n\n<script>\n// import TeacherCard from \"@/components/TeacherCard.vue\";\nimport { mapGetters, mapMutations } from \"vuex\";\n// import Notifications from \"../components/Notifications\";\n\nexport default {\n  components: {\n    // Notifications,\n    // TeacherCard\n  },\n  props: {\n    curators: {\n      type: Array,\n      required: true\n    }\n  },\n  data() {\n    return {\n      teacher: null,\n      show_favorite: false,\n      filters: {\n        native_speaker: null,\n        date: null,\n        time: null\n      },\n      tests: null,\n      theaches: [],\n      options: [\n        {\n          val: \"all\",\n          text: \"Все курсы\"\n        },\n        {\n          val: \"my\",\n          text: \"Мои курсы\"\n        }\n      ],\n      lessons: []\n    };\n  },\n  computed: {\n    ...mapGetters(\"user\", [\n      \"user_role\",\n      \"favorite_teachers\",\n      \"blocked_teachers\"\n    ]),\n    ...mapGetters([\"groups\"]),\n    role() {\n      return this.user_role === \"default\";\n    },\n    curators_list() {\n      if (this.isOnlyFavorites) {\n        const arr = this.favorite_teachers.map(f => f.teacher);\n        return this.$store.getters[\"curators/curators_array\"].filter(curator =>\n          arr.includes(curator.id)\n        );\n      }\n      return this.$store.getters[\"curators/curators_array\"];\n\n      // if (!this.blocked_teacher && !this.favorite_teachers) return [];\n      // let curators_id = this.isOnlyFavorites\n      //   ? this.favorite_teachers\n      //   : this.blocked_teachers;\n      // return this.$store.getters['curators/curators_array'].find(curator => {\n      //   if(this.isOnlyFavorites) return curators_id.includes(curator.id)\n      //   console.log(curator)\n      //   // for (let curator of curators_id) {\n      //   //   // if(curator.teacher !== teacher.id && !this.isOnlyFavorites) {\n      //   //   //   return teacher\n      //   //   // }\n      //   //   // if (curator.teacher !== teacher.id) {\n      //   //   //   return teacher;\n      //   //   // }\n      //   //   // if (curator.teacher !== teacher.id && !this.isOnlyFavorites) {\n      //   //   //   return teacher;\n      //   //   // }\n      //   //   // if (curator.teacher === teacher.id && this.isOnlyFavorites) {\n      //   //   //   return teacher;\n      //   //   // }\n      //   // }\n      // });\n    },\n    isOnlyFavorites() {\n      return this.show_favorite;\n    }\n  },\n  methods: {\n    ...mapMutations([\"SHOW_MODAL\", \"MODAL_DATA\"]),\n    openModal(lesson) {\n      this.SHOW_MODAL(\"group-modal\");\n      this.MODAL_DATA(lesson);\n    },\n    teacher_type(type) {\n      this.filters.native_speaker =\n        type === \"type-1\" ? true : type === \"all\" ? null : false;\n      this.filter_request();\n    },\n    change_date(date) {\n      this.filters.date = date;\n      this.filter_request();\n    },\n    change_time(time) {\n      this.filters.time = time;\n      this.filter_request();\n    },\n    async filter_request() {\n      const send_data = {};\n      if (this.check_filters) {\n        if (this.filters.native_speaker)\n          send_data.native_speaker = this.filters.native_speaker;\n        if (this.filters.date) send_data.date = this.filters.date;\n        if (this.filters.time) send_data.time = this.filters.time;\n      }\n      Object.values(send_data).length > 0\n        ? await this.$store.dispatch(\"curators/filter_curators\", send_data)\n        : await this.$store.dispatch(\"curators/get_curators\");\n    },\n    check_filters() {\n      return (\n        Object.values(this.filters).filter(e => !this.is_null(e)).length > 0\n      );\n    },\n    is_null(value) {\n      return value === null;\n    },\n    show_favor_curators() {\n      this.show_favorite = !this.show_favorite;\n    }\n  },\n  created() {\n    this.$store.dispatch(\"get_group\");\n  },\n  mounted() {\n    function getUniqueListBy(arr, key) {\n      console.log(key);\n      return [...new Map(arr.map(item => [item[key], item])).values()];\n    }\n    // function are_equal(obj1,obj2) {\n    //   return JSON.stringify(obj1)==JSON.stringify(obj2);\n    // }\n    const list = this.groups.map(g => {\n      return [...g.times];\n    });\n    const lessons = this.groups.map(g => {\n      return {\n        ...g.times[0].group_lesson,\n        teacher: g.times[0].teacher,\n        link_to_lesson: g.times[0].link_to_lesson\n      };\n    });\n    // console.log([...new Set(...getUniqueListBy(list, \"id\")).map(i => i.group_lesson.id)]);\n    this.lessons = getUniqueListBy(lessons, \"id\");\n  }\n  // created() {\n  //   console.log(this.curators)\n  // }\n};\n</script>\n\n<style lang=\"scss\">\n.group-wrapper {\n  display: grid;\n  grid-template-columns: 1fr 1fr;\n  grid-gap: 30px;\n  .group-card {\n    cursor: pointer;\n    background: #f7f7f7;\n    border-radius: 2px;\n    padding: 30px;\n    display: flex;\n    justify-content: center;\n    flex-direction: column;\n    align-items: center;\n\n    img {\n      margin-bottom: 20px;\n    }\n\n    .name {\n      font-size: 18px;\n      color: #251c1c;\n      @extend %text-middle;\n      font-weight: 700;\n      margin-bottom: 20px;\n    }\n\n    .title {\n      margin-bottom: 20px;\n      @extend %text-middle;\n      font-weight: 700;\n      color: #251c1c;\n      font-size: 30px;\n      text-align: center;\n    }\n\n    hr {\n      height: 1px;\n      background-color: #d8d8d8;\n      width: 200px;\n      color: #d8d8d8;\n      border-color: #d8d8d8;\n      border: 0;\n      margin-bottom: 20px;\n    }\n\n    .goToLesson {\n      background: #39b100;\n      padding: 14px 20px;\n      font-size: 16px;\n      color: #fff;\n      line-height: 21px;\n      @extend %text-middle;\n    }\n\n    .schedule {\n      @extend %text-middle;\n      font-size: 16px;\n      margin-bottom: 10px;\n    }\n\n    .times {\n      @extend %text-big;\n      font-size: 18px;\n      margin-bottom: 10px;\n    }\n  }\n}\n.teachers {\n  padding-top: 44px;\n  padding-left: 30px;\n  max-width: 921px;\n  &__not-found {\n    @extend %h3-title-bold;\n    color: $gray;\n  }\n  &__wrap {\n    &_title {\n      @extend %df;\n      margin-bottom: rem(58);\n    }\n\n    &_filters {\n      @extend %df;\n      // align-items: flex-start;\n      justify-content: space-between;\n\n      margin-bottom: 32px;\n    }\n\n    &__tabs {\n      a {\n        padding: 7px 53px;\n        color: #fff;\n        font-size: 16px;\n        border: 1px solid #39b100;\n        color: #39b100;\n        @extend %btn-text;\n        &:hover {\n          background: #39b100;\n          color: #fff;\n        }\n      }\n    }\n  }\n\n  &__title {\n    @extend %h2-title-bold;\n    color: $black;\n    margin-left: 14px;\n\n    &-img {\n      width: 44px;\n      height: 36px;\n    }\n  }\n\n  &__filters-wrap {\n    @extend %df;\n    align-items: flex-end;\n    @extend %jcfs;\n    width: auto !important;\n  }\n\n  &__my {\n    @extend %simple-select;\n    width: 187px;\n    @extend %btn-text;\n    color: $black;\n    &.active {\n      color: $green;\n      border-color: $green;\n    }\n    span {\n      @extend %db;\n      text-align: center;\n      line-height: rem(35);\n    }\n\n    &:hover {\n      color: $green;\n    }\n  }\n}\n\n@include bp(1360px) {\n  .teachers {\n    padding-right: 30px;\n  }\n}\n\n@include bp(1100px) {\n  .teachers {\n    padding-top: 57px;\n    padding-left: 0;\n    max-width: 750px;\n    margin: 0 auto;\n    padding-right: 0;\n\n    &__filters-wrap {\n      width: 402px;\n    }\n  }\n}\n\n@include bp(766px) {\n  .teachers {\n    max-width: 320px;\n\n    &__title {\n      font-size: rem(20);\n      line-height: rem(25);\n    }\n\n    &__filters-wrap {\n      width: 100%;\n      position: relative;\n      z-index: 5;\n\n      .select {\n        width: 161px;\n        height: 30px;\n\n        &__wrapper {\n          height: 30px;\n        }\n\n        &__wrap-val {\n          padding-left: 15px;\n          padding-right: 5px;\n          height: 30px;\n        }\n\n        &__value {\n          font-size: rem(12);\n        }\n\n        &__optios {\n          padding-left: 15px;\n        }\n\n        &__option-btn {\n          font-size: rem(12);\n        }\n      }\n\n      .datepiker {\n        width: 66px;\n        height: 30px;\n\n        &__val {\n          font-size: rem(12);\n        }\n      }\n\n      .timepicker {\n        width: 75px;\n        height: 30px;\n\n        &__val {\n          font-size: rem(12);\n        }\n      }\n    }\n\n    &__my {\n      width: 100%;\n      height: 30px;\n      font-size: rem(12);\n      line-height: rem(14);\n      margin-top: 11px;\n      display: flex;\n      justify-content: center;\n      align-items: center;\n      span {\n        line-height: 16px;\n      }\n    }\n\n    &__wrap {\n      &_filters {\n        width: 100%;\n        flex-direction: column;\n      }\n    }\n  }\n}\n</style>\n"]}]}