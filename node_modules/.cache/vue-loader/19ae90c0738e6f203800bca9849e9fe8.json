nd %text-very-small;\n    color: $red;\n    left: 0;\n    bottom: -25px;\n  }\n  &.error {\n    border-color: $red;\n    .c-input__field {\n      color: $red;\n      &-icon {\n        color: $red;\n      }\n    }\n  }\n  &__wrap-field {\n    order: 2;\n    width: 100%;\n    height: 100%;\n  }\n\n  &__wrap-icon {\n    order: 1;\n    .user-svg {\n      margin-left: 7px;\n    }\n    &_user {\n      width: rem(30);\n      height: rem(18);\n    }\n    &_email {\n      width: rem(30);\n      height: rem(16);\n    }\n  }\n\n  &__field {\n    @extend %dib;\n    width: 100%;\n    height: 100%;\n    @extend %text-middle;\n    @extend %input-reset;\n    color: $black;\n    padding-left: 6px;\n    color: $black;\n\n    &-icon {\n      position: absolute;\n      top: 50%;\n      transform: translateY(-50%);\n      // left: rem(27);\n      &.icon {\n        fill: transparent;\n        stroke: currentColor;\n      }\n      width: inherit;\n      height: inherit;\n    }\n  }\n}\n</style>\n"]}]}