hboard__chart\n\n  &__chart {\n    width: rem(430);\n    height: rem(118);\n    /*<!--background-color: $gray;-->*/\n  }\n}\n@include bp(1100px) {\n  .c-teacher-dashboard {\n    &__chart {\n      width: 362px;\n    }\n    &__info-item {\n      margin-right: 20px;\n    }\n  }\n}\n@include bp(766px) {\n  .c-teacher-dashboard {\n    flex-direction: column;\n    height: auto;\n    padding: 20px 10px;\n    &__chart {\n      width: 100%;\n      margin-top: 35px;\n    }\n  }\n}\n</style>\n"]}]}