       v-model=\"new_password\"\n                             :error=\"error_new_pass\" />\n          <button class=\"c-reset-password__save-password\">\n            Сохранить пароль\n          </button>\n        </form>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"ResetPassword\",\n  data() {\n    return {\n      old_password: null,\n      new_password: null,\n      error_old_pass: false,\n      error_new_pass: false\n    }\n  },\n  methods: {\n    onSubmit() {\n      if(this.old_password && this.new_password) {\n        if(this.old_password.length < 6 && this.new_password.length < 6 && this.old_password !== this.new_password) {\n          this.error_old_pass = true\n          this.error_new_pass = true\n          setTimeout(() => {\n            this.error_old_pass = false\n            this.error_new_pass = false\n          }, 3000)\n          return\n        }\n      } else {\n        this.error_old_pass = true\n        this.error_new_pass = true\n        setTimeout(() => {\n          this.error_old_pass = false\n          this.error_new_pass = false\n        }, 3000)\n        return\n      }\n      const sendingData = {\n        new_password: this.new_password,\n        remember_token: this.$route.params.token\n      }\n\n      this.$store.dispatch('auth/reset_password',sendingData)\n    }\n  },\n};\n</script>\n\n<style lang=\"scss\">\n.c-reset-password {\n  &__container {\n    max-width: rem(1035);\n    margin: 0 auto;\n    @extend %h100;\n    text-align: center;\n  }\n\n  &__content {\n    height: calc(100vh - 64px);\n    margin: 0 auto;\n    @extend %df;\n    @extend %jcc;\n    @extend %fdc;\n  }\n\n  &__header {\n    padding-top: rem(18);\n    &-items {\n      @extend %df;\n      @extend %jcsb;\n    }\n  }\n  &__form {\n    @extend %h100;\n    @extend %df;\n    @extend %jcc;\n    @extend %fdc;\n    width: rem(340);\n    margin: 0 auto;\n    &-field {\n      margin-bottom: rem(10);\n    }\n  }\n\n  &__auth {\n    &-login {\n      @extend %btn-border-grey;\n    }\n    &-register {\n      @extend %btn-all-green;\n      padding: 6px 25px;\n      margin-left: rem(15);\n    }\n  }\n\n  &__save-password {\n    @extend %btn-danger;\n    width: 100%;\n    margin-top: rem(37);\n  }\n\n  &__tag {\n    margin-bottom: rem(41);\n    @extend %h2-title-regular;\n  }\n}\n</style>\n"]}]}