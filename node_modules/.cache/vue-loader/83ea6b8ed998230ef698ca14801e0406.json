"student.time_to\"\n          :isPassed=\"!!student.link_to_lesson\"\n          :link_to_lesson=\"student.link_to_lesson\"\n          :link_to_homework=\"student.link_to_homework\"\n          :lesson_type=\"student.lesson_type\"\n          :lesson_comment=\"student.lesson_comment\"\n        />\n        <!-- @showUser=\"showUserModal\" -->\n      </template>\n      <template v-else>\n        <div class=\"v-new-lessons__empty\">\n          <div class=\"v-new-lessons__wrapper\">\n            <figure class=\"v-new-lessons__ill\">\n              <img src=\"/img/exam.svg\" alt=\"\" />\n            </figure>\n            <div class=\"v-new-lessons__content\">\n              <h3 class=\"v-new-lessons__em-title\">Новых занятий нет</h3>\n              <!-- <p class=\"v-new-lessons__em-text\">text</p> -->\n            </div>\n          </div>\n        </div>\n      </template>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapMutations, mapGetters } from \"vuex\";\nimport Student from \"@/components/NewTeacherLessons/Student.vue\";\nimport { get_curator_users } from \"@/mixins/mixins.js\";\nexport default {\n  components: {\n    Student\n  },\n  mixins: [get_curator_users],\n  data() {\n    return {};\n  },\n  computed: {\n    ...mapGetters(\"user\", [\n      \"curator_not_confirmed_lessons\",\n      \"curator_confirmed_lessons\"\n    ])\n  },\n  methods: {\n    ...mapMutations([\"SHOW_MODAL\"]),\n    // Получает id занятия id\n    showUserModal() {\n      this.SHOW_MODAL(\"student-modal\");\n    },\n    // Получает id юзера id\n    showLessonModal() {\n      this.SHOW_MODAL(\"new-teacher-lesson\");\n    }\n  }\n};\n</script>\n\n<style lang=\"scss\">\n.v-new-lessons {\n  @extend %pagept;\n  max-width: rem(892);\n  // .v-new-lessons__wrap\n\n  &__wrap {\n    // .v-new-lessons__wrap_title\n\n    &_title {\n    }\n\n    // .v-new-lessons__wrap_users\n\n    &_users {\n    }\n  }\n\n  // .v-new-lessons__title\n\n  &__title {\n    @extend %h3-title-bold;\n    color: $gray;\n    margin-bottom: rem(32);\n  }\n  &__wrapper {\n    @extend %df;\n  }\n  &__ill {\n    max-width: 200px;\n  }\n  &__content {\n    margin-left: 50px;\n  }\n  &__em {\n    &-title {\n      @extend %h3-title-bold;\n      color: $gray;\n      margin-bottom: 15px;\n    }\n    &-text {\n      @extend %text-small;\n      color: $gray;\n    }\n  }\n}\n@include bp(1100px) {\n  .v-new-lessons {\n    max-width: 750px;\n    margin: 0 auto;\n  }\n}\n@include bp(766px) {\n  .v-new-lessons {\n    max-width: 320px;\n    padding: 50px 0 0 0;\n    &__title {\n      font-size: rem(16);\n    }\n    &__wrapper {\n      flex-direction: column;\n    }\n    &__content {\n      margin-left: 0;\n    }\n  }\n}\n</style>\n"]}]}