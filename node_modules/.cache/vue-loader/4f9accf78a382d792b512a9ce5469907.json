\"\n            :data=\"items\"\n            :marks=\"true\"\n            v-model=\"value\"\n    />\n  </div>\n</template>\n\n<script>\n  import \"vue-slider-component/theme/default.css\";\n  import VueSlider from \"vue-slider-component\";\n\n  export default {\n    name: \"TimeOffSliderTeacher\",\n    components: {\n      VueSlider\n    },\n    props: {\n      hour: {\n        type: Number,\n        required: true\n      },\n      minutesFrom: {\n        type: Number,\n        required: true\n      },\n      minutesTo: {\n        type: Number,\n        required: true\n      },\n      is_edit: {\n        type: Boolean,\n      },\n      date: {\n        type: String\n      }\n    },\n    computed: {\n      items() {\n        const minutes = [];\n        const hour = this.hour < 10 ? `0${this.hour}` : this.hour;\n\n        for (let j = this.minutesFrom; j <= this.minutesTo; j += 5) {\n          if (j === 60) {\n            continue;\n          }\n          const minute = j < 10 ? `0${j}` : j;\n\n          minutes.push(`${hour}:${minute}`);\n        }\n\n        return minutes;\n      },\n      timeEnd() {\n        return this.items.slice(-1)[0];\n      },\n      componentStyle() {\n        const grid = `${this.minutesFrom === 0 ? 1 : this.minutesFrom} / ${\n          this.minutesTo\n        }`;\n        return `grid-column: ${grid}`;\n      },\n      classNameForSmall() {\n        return this.minutesTo < 60 ? `c-teacher-calendar__timeline_small` : \"\";\n      }\n    },\n    data() {\n      return {\n        value: ''\n      };\n    },\n    methods: {\n      mouserOverEvent() {\n        this.$emit(\"hover\", null);\n      },\n    }\n  };\n</script>\n"]}]}