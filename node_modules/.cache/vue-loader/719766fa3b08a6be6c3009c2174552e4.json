_title {\n      @extend %df;\n      margin-bottom: rem(58);\n    }\n\n    &_filters {\n      @extend %df;\n      align-items: flex-start;\n      @extend %jcsb;\n\n      margin-bottom: 32px;\n    }\n  }\n\n  &__title {\n    @extend %h2-title-bold;\n    color: $black;\n    margin-left: 14px;\n\n    &-img {\n      width: 44px;\n      height: 36px;\n    }\n  }\n\n  &__filters-wrap {\n    @extend %df;\n    align-items: flex-start;\n    @extend %jcfs;\n    width: 392px;\n  }\n\n  &__my {\n    @extend %simple-select;\n    width: 187px;\n    @extend %btn-text;\n    color: $black;\n    &.active {\n      color: $green;\n      border-color: $green;\n    }\n    span {\n      @extend %db;\n      text-align: center;\n      line-height: rem(35);\n    }\n\n    &:hover {\n      color: $green;\n    }\n  }\n}\n\n@include bp(1360px) {\n  .teachers {\n    padding-right: 30px;\n  }\n}\n\n@include bp(1100px) {\n  .teachers {\n    padding-top: 57px;\n    padding-left: 0;\n    max-width: 750px;\n    margin: 0 auto;\n    padding-right: 0;\n\n    &__filters-wrap {\n      width: 402px;\n    }\n  }\n}\n\n@include bp(766px) {\n  .teachers {\n    max-width: 320px;\n\n    &__title {\n      font-size: rem(20);\n      line-height: rem(25);\n    }\n\n    &__filters-wrap {\n      width: 100%;\n      position: relative;\n      z-index: 5;\n\n      .select {\n        width: 161px;\n        height: 30px;\n\n        &__wrapper {\n          height: 30px;\n        }\n\n        &__wrap-val {\n          padding-left: 15px;\n          padding-right: 5px;\n          height: 30px;\n        }\n\n        &__value {\n          font-size: rem(12);\n        }\n\n        &__optios {\n          padding-left: 15px;\n        }\n\n        &__option-btn {\n          font-size: rem(12);\n        }\n      }\n\n      .datepiker {\n        width: 66px;\n        height: 30px;\n\n        &__val {\n          font-size: rem(12);\n        }\n      }\n\n      .timepicker {\n        width: 75px;\n        height: 30px;\n\n        &__v